doctype html
html
 head
    link(href='svg.css', rel='stylesheet')
    title= pageName
  body
   - var svgattributes = {'xmlns' : 'http://www.w3.org/2000/svg', 'version' : '1.1'};
   svg&attributes(svgattributes)
    for block in blocks
     // block
     g(transform='translate(#{block.screenx} #{block.screeny})')
      image(xlink:href='#{block.type.toString()}.svg')
     